{
  "@context": {
    "@version": 1.1,
    "@protected": true,
    "schema": "https://schema.org/",
    "ex": "https://example.org/example#",

    "ParticipantCredential": {
      "@id": "ex:ParticipantCredential",
      "@context": {
        "description": "schema:description",
        "identifier": "schema:identifier",
        "name": "schema:name",
        "image": "schema:image"
      }
    },

    "participantDetails": {
      "@id": "ex:ParticipantDetails",
      "@context": {
        "name": "schema:name",
        "businessPartnerNumber": "schema:Text",
        "zipCode": "schema:postalCode",
        "countryAlpha2Code": "schema:Country"
      }
    },

    "identityDetails": {
      "@id": "ex:IdentityDetails",
      "@context": {
        "did": "schema:Text",
        "uniqueIds": {
          "@id": "ex:UniqueIds",
          "@context": {
            "_type": "schema:Text",
            "value": "schema:Text"
          }
        }
      }
    }
  }
}
