{
  "@context": {
    "@version": 1.1,
    "id": "@id",
    "type": "@type",
    "emp": "https://spherity.github.io/schemas/testing/employee.jsonld#",
    "termsOfUse": "https://spherity.github.io/schemas/testing/termsOfUsejsonld#",
    "schema": "https://schema.org/",
    "Employee": "emp:Employee",
    "person": {
      "@id": "schema:Person",
      "@type": "@id",
      "@context": {
        "did": {
          "@id": "emp:personDid",
          "@type": "@id"
        },
        "email": {
          "@id": "emp:employeeEmail",
          "@type": "schema:email"
        },
        "givenName": {
          "@id": "emp:givenName",
          "@type": "schema:Text"
        },
        "familyName": {
          "@id": "emp:familyName",
          "@type": "schema:Text"
        },
        "birthDate": {
          "@id": "emp:birthDate",
          "@type": "schema:Date"
        },
        "birthPlace": {
          "@id": "emp:birthPlace",
          "@type": "schema:addressCountry"
        },
        "gender": {
          "@id": "emp:birthDate",
          "@type": "schema:Text"
        }
      }
    },
    "departement": {
      "@id": "schema:Organization",
      "@type": "@id",
      "@context": {
        "did": {
          "@id": "emp:departementDid",
          "@type": "@id"
        },
        "legalName": {
          "@id": "emp:legalName",
          "@type": "schema:Text"
        },
        "parentOrganization": {
          "@id": "schema:Organization",
          "@type": "schema:Organization",
          "@context": {
            "legalName": {
              "@id": "emp:legalName",
              "@type": "schema:Text"
            }
          }
        },
        "address": {
          "@id": "emp:departementAddress",
          "@type": "schema:text"
        },
        "email": {
          "@id": "emp:departementEmail",
          "@type": "schema:email"
        }
      }
    }
  }
}