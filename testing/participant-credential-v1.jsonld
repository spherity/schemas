{
  "@context": {
    "@version": 1.1,
    "@protected": true,
    "id": "@id",
    "type": "@type",
    "ns": "http://carla.project#",
    "schema": "https://schema.org/",

    "ParticipantCredential": "ns:ParticipantCredential",

    "participantDetails": {
      "@id": "ns:participantDetails",
      "@context": {
        "@protected": true,

        "name": {
          "@id": "ns:participantDetails_name",
          "@type": "schema:name"
        },

        "city": {
          "@id": "ns:participantDetails_city",
          "@type": "schema:text"
        },

        "street": {
          "@id": "ns:participantDetails_street",
          "@type": "schema:text"
        },

        "bpn": {
          "@id": "ns:participantDetails_bpn",
          "@type": "schema:text"
        },

        "region": {
          "@id": "ns:participantDetails_region",
          "@type": "schema:text"
        },

        "zipCode": {
          "@id": "ns:participantDetails_zipCode",
          "@type": "schema:text"
        },

        "country": {
          "@id": "ns:participantDetails_country",
          "@type": "schema:text"
        },

        "countryAlpha2Code": {
          "@id": "ns:participantDetails_countryAlpha2Code",
          "@type": "schema:text"
        }
      }
    },

    "identityDetails": {
      "@id": "ns:identityDetails",
      "@context": {
        "@protected": true,
        "did": {
          "@id": "ns:identityDetails_did",
          "@type": "schema:identifier"
        },
        "uniqueIds": {
          "@id": "ns:uniqueIds",
          "@container": "@set",
          "@context": {
            "type": {
              "@id": "ns:identityDetails_uniqueId_type",
              "@type": "schema:text"
            },
            "value": {
              "@id": "ns:identityDetails_uniqueId_value",
              "@type": "schema:text"
            }
          }
        }
      }
    }
  }
}
